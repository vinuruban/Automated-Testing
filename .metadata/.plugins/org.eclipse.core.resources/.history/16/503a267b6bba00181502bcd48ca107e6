package com.qa.BlackJack;

import static org.junit.Assert.*;

import org.junit.Test;

public class AppTest 
{

	@Test
	public void test() {

	assertEquals("What do you mean?", 10, blackJack(10, 22));
	assertEquals("What do you mean?", 9, blackJack(22, 9));
	assertEquals("What do you mean?", 0, blackJack(22, 22));
	assertEquals("What do you mean?", 10, blackJack(10, 7));
	assertEquals("What do you mean?", 19, blackJack(1, 19));

	}

	
}
